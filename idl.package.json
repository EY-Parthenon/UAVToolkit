{
    "name": "UAVToolkit",
    "version": "2.1",
    "license": "MIT",
    "author": "Zachary Norman",
    "description": "A tool used to preprocess multispectral multi-lens imagery.",
    "entry_point": "uav_toolkit",
    "build_types": [
        "idl"
    ],
    "build_ignore": [
        "dist",
        "docs"
    ],
    "scripts": {
        "add-header": [
            [
                "self.addFileHeaders, /OVERWRITE, ['(c) 2018 Harris Geospatial Solutions, Inc.',",
                "'',",
                "'Licensed under MIT. See LICENSE.txt for additional details and information.']"
            ]
        ],
        "doc": [
            [
                "self.document, /CLEAR_PREVIOUS,",
                "FOOTER=['&copy; 2018 Harris Geospatial Solutions, Inc.',",
                "'', 'Licensed under MIT. See LICENSE.txt for additional details and information.']"
            ]
        ]
    },
    "dependencies": {
        "github-se-awesomeenvitools": {
            "version": "^v0.0.2",
            "url": "hgsolutions/SE-AwesomeENVITools"
        },
        "github-BridgeIt": {
            "version": "^v1.0.0",
            "url": "interactive-data-language/BridgeIt"
        }
    }
}
